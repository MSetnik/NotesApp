{
  "expo": {
    "name": "NotesApp",
    "slug": "NotesApp",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/notes-icon4.png",
    "splash": {
      "image": "./assets/splashscreen.png",
      "resizeMode": "contain" ,
      "backgroundColor": "#F2F2F2"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.msetnik.NotesApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/notes-icon4.png",
        "backgroundColor": "#88B6E1"
      },
      "package": "com.msetnik.NotesApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
